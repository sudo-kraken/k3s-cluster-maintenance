{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    "replacements:k8s-registry-move",
    ":automergePr",
    ":automergePatch",
    ":automergeDigest",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":timezone(Europe/London)",
    "github>sudo-kraken/k3s-cluster-maintenance//.github/renovate/ansible.json5",
    "github>sudo-kraken/k3s-cluster-maintenance//.github/renovate/devContainers.json5",
    "github>sudo-kraken/k3s-cluster-maintenance//.github/renovate/labels.json5",
    "github>sudo-kraken/k3s-cluster-maintenance//.github/renovate/semanticCommits.json5",
  ],
  dependencyDashboardTitle: "Renovate Dashboard ðŸ¤–",
  suppressNotifications: ["prEditedNotification", "prIgnoreNotification"],
  rebaseWhen: "conflicted",
  pruneStaleBranches: true,
  pruneBranchAfterAutomerge: true,
  prConcurrentLimit: 5,
  schedule: ["before 9pm on friday"],
  labels: ["dependencies"],
  minimumReleaseAge: "7 days",

  packageRules: [
    {
      matchManagers: ["github-actions"],
      minimumReleaseAge: null,
    },
  ],
}
